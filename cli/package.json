{
  "name": "ammplify-cli",
  "version": "0.1.0",
  "description": "CLI for interacting with the Ammplify protocol",
  "type": "module",
  "main": "./dist/index.js",
  "bin": {
    "ammplify": "./dist/index.js"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "dev": "tsx src/index.ts",
    "build": "tsup src/index.ts --format esm --dts --clean",
    "prebuild": "tsx src/abi/extract-abis.ts",
    "extract-abis": "tsx src/abi/extract-abis.ts",
    "test": "vitest run",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/itos-finance/AmmplifyPublic.git",
    "directory": "cli"
  },
  "license": "MIT",
  "keywords": [
    "ammplify",
    "defi",
    "cli",
    "viem",
    "liquidity"
  ],
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "chalk": "^5.3.0",
    "cli-table3": "^0.6.5",
    "commander": "^12.1.0",
    "dotenv": "^16.4.5",
    "ora": "^8.1.0",
    "prompts": "^2.4.2",
    "viem": "^2.21.0"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "@types/prompts": "^2.4.9",
    "tsup": "^8.3.0",
    "tsx": "^4.19.0",
    "typescript": "^5.6.0",
    "vitest": "^2.1.9"
  }
}
